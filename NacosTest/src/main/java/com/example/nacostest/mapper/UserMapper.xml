<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.nacostest.mapper.UserMapper">
    <select id="getUserInfoByName" resultType="com.example.nacostest.entity.User">
        select id,name,pwd,rid from user where name = #{name}
    </select>
</mapper>